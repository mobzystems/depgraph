{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "party-reasons-tauri",
    "version": "0.6.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "ask": true,
        "open": true
      },
      "shell": {
        "all": false,
        "open": true,
        "execute": true,
        "scope": [ {"name": "cmd", "cmd": "cmd", "args": true, "sidecar": false }]
      },
      "fs": {
        "all": true
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": ["deb"],
      "identifier": "depgraph.mobzystems.com",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Dependency Graph",
        "width": 1200,
        "height": 800
      }
    ]
  }
}
